        <div id="pins" %>
        <% @pins.each do |pin| %>
        <%= image_tag pin.image.url (:medium) %>
        <%= pin.description %>
        <%= pin.string %>
        <%= link_to 'Show', pin %>
        <%= link_to 'Edit', edit_pin_path(pin) %>
        <%= link_to 'Destroy', pin, method: :delete, data: { confirm: 'Are you sure?' } %>
    <% end %>
    </div>
